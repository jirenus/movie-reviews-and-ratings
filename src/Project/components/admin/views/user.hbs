<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">USERS</h4>

            </p>
            <div class="table-responsive">
                <table class="table table-striped">
                    <thead>
                    <tr>
                        <th>User</th>
                        <th>Username</th>
                        <th>Name</th>
                        <th>Email</th>
                        <th>Status</th>
                        <!--                        <th>Last access</th>-->
                        <th></th>

                    </tr>
                    </thead>
                    <tbody>
                    {{#each userList}}
                        {{#if userType}}

                        {{else}}
                            <tr>
                                <td class="py-1">
                                    <img src="data:image/jpeg;base64," alt="image" />
                                </td>
                                <td>{{username}}</td>
                                <td>{{name}}</td>
                                <td>{{email}}</td>
                                <td>
                                    {{#if status}}
                                        <div class="badge badge-inverse-success"> normal </div>
                                    {{else}}
                                        <div class="badge badge-inverse-success"> locked </div>
                                    {{/if}}
                                </td>
                                <td>
                                    <div class="dropdown">
                                        <button class="btn btn-warning dropdown-toggle" type="button" id="dropdownMenuIconButton1" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                            <i class="mdi mdi-earth"></i>
                                        </button>
                                        <div class="dropdown-menu" aria-labelledby="dropdownMenuIconButton1">
                                            <h6 class="dropdown-header">Settings</h6>
                                            <a class="dropdown-item" href="/admin/userProfile/{{_id}}">View</a>
                                            <a class="dropdown-item" href="/admin/updateProfile/{{_id}}">Edit</a>
                                            <a class="dropdown-item" href="/admin/user/delete/{{_id}}">Delete</a>
                                            <a class="dropdown-item" href="/admin/user/lock/{{_id}}">Lock</a>
                                            <a class="dropdown-item" href="/admin/user/unlock/{{_id}}">Unlock</a>
                                        </div>
                                    </div>
                                </td>
                            </tr>
                        {{/if}}
                    {{/each}}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>